<?xml version="1.0" encoding="utf-8"?>








<RelativeLayout android:id="@+id/RelativeLayout01" android:layout_width="fill_parent" android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"><Button android:text="Reset" android:id="@+id/Reset" android:layout_height="wrap_content" android:layout_width="fill_parent" android:layout_alignParentBottom="true"></Button>

<Button android:text="Exercise" android:id="@+id/Start" android:layout_above="@+id/Reset" android:layout_height="wrap_content" android:layout_centerHorizontal="true" android:layout_alignParentTop="false" android:layout_width="fill_parent"></Button>

<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerHorizontal="true" android:text="00" android:id="@+id/Timer" android:layout_centerInParent="true" android:layout_centerVertical="true"></TextView><TextView android:layout_centerHorizontal="true" android:text="Six Minute Abs Timer" android:id="@+id/Banner" android:layout_height="wrap_content" android:layout_width="wrap_content"></TextView><DigitalClock android:layout_above="@+id/Button02" android:layout_width="wrap_content" android:text="@+id/DigitalClock01" android:layout_height="wrap_content" android:layout_centerHorizontal="true" android:id="@+id/DigitalClock01" android:visibility="gone"></DigitalClock>


</RelativeLayout>
